{
  "name": "clm-position-test",
  "version": "1.0.0",
  "description": "API REST using NodeJS and KOA framework, typescript, class-validators. Middlewares JWT, CORS, MongoDB database, Winston Logger. Swagger API docs",
  "main": "dist/server.js",
  "scripts": {
    "watch-node": "nodemon dist/server.js",
    "watch-server": "nodemon --watch src -e ts,tsx --exec ts-node src/server.ts --trace-warnings",
    "build-ts": "tsc",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve": "node dist/server.js",
    "build": "npm run tslint && npm run build-ts && npm run copy-static-assets",
    "start": "npm run serve"
  },
  "engines": {
    "node": "10.x.x",
    "npm": "6.x.x"
  },
  "author": "David Misael Villegas Sandoval",
  "license": "MIT",
  "homepage": "https://github.com/misaelvillegas97/node_microservice_koa_mongo",
  "keywords": [
    "typescript",
    "koa",
    "koa2",
    "jwt",
    "winston",
    "boilerplate",
    "helmet",
    "api rest",
    "heroku",
    "cors",
    "github actions"
  ],
  "repository": "github:misaelvillegas97/node_microservice_koa_mongo",
  "dependencies": {
    "@koa/cors": "3.0.0",
    "@types/uuid": "3.4.6",
    "axios": "^0.21.1",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "3.5.5",
    "class-validator": "0.9.1",
    "config": "3.2.4",
    "cron": "1.7.2",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-helmet": "4.2.0",
    "koa-jwt": "3.3.2",
    "koa-router": "7.4.0",
    "koa-swagger-decorator": "1.5.16",
    "mongoose": "5.7.1",
    "mongoose-paginate-v2": "^1.3.18",
    "reflect-metadata": "0.1.12",
    "saslprep": "^1.0.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "3.5.18",
    "@types/config": "0.0.35",
    "@types/cron": "1.7.1",
    "@types/koa": "2.0.49",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-helmet": "3.1.2",
    "@types/koa-jwt": "3.3.0",
    "@types/koa-router": "7.0.42",
    "@types/koa__cors": "2.2.3",
    "@types/mongoose": "5.5.18",
    "@types/node": "12.0.12",
    "@types/shelljs": "0.8.0",
    "nodemon": "1.19.1",
    "shelljs": "0.8.2",
    "ts-node": "8.4.1",
    "tslint": "5.18.0",
    "typescript": "3.6.3"
  }
}
